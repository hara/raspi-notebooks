    "from time import sleep\n",
    "from RPi import GPIO\n",
    "\n",
    "GPIO.setmode(GPIO.BCM)\n",
    "GPIO.setup(4, GPIO.OUT)\n",
    "\n",
    "for i in range(5):\n",
    "    GPIO.output(4, GPIO.HIGH)\n",
    "    sleep(0.5)\n",
    "    GPIO.output(4, GPIO.LOW)\n",
    "    sleep(0.5)\n",
    "\n",
    "GPIO.cleanup()"
   ]
